# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

target 'Third-party-framework' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Alamofire' 
  pod 'SwiftyJSON'
  pod 'Kingfisher'
  pod 'SDWebImage'	
  pod 'FLAnimatedImage'
  pod 'SnapKit', '~> 4.0.0'
  pod 'XSLPhotoBrowser', '~> 0.1.0'
  pod 'Hero'
  target 'Third-party-frameworkTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Third-party-frameworkUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
